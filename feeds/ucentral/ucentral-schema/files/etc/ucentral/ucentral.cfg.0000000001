{
	"uuid": 1,
	"interfaces": [
		{
			"name": "WAN",
			"role": "upstream",
			"ethernet": [
				{
					"select-ports": [
						"WAN*"
					]
				}
			],
			"ipv4": {
				"addressing": "dynamic"
			}
		},
		{
			"name": "WAN-wifi",
			"role": "upstream",
			"ipv4": {
				"addressing": "dynamic"
			},
			"metric": 6,
			"ssids": [
				{
					"name": "OpenWifi-onboarding",
					"wifi-bands": [
						"2G"
					],
					"bss-mode": "sta",
					"encryption": {
						"proto": "wpa2",
						"ieee80211w": "required"
					},
					"certificates": {
						"use-local-certificates": true
					}
				}
			]
		},
		{
			"name": "LAN",
			"role": "downstream",
			"services": [ "ssh" ],
			"ethernet": [
				{
					"select-ports": [
						"LAN*"
					]
				}
			],
			"ipv4": {
				"addressing": "static",
				"subnet": "192.168.1.1/24",
				"dhcp": {
					"lease-first": 10,
					"lease-count": 100,
					"lease-time": "6h"
				}
			}
		}
	],
	"services": {
		"ssh": {
			"port": 22
		}
	}
}
